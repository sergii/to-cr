<style>
  table tbody td {
    vertical-align: top;
    padding: 0
  }
</style>

<h1>User Sessions</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Scope</th>
      <th>Strategy</th>
      <th>Success</th>
      <th>Failure reason</th>
      <th>User type</th>
      <th>Context</th>
      <th>IP</th>
      <th>User agent</th>
      <th>Referrer</th>
      <th>City</th>
      <th>Region</th>
      <th>Country</th>
      <th>Login</th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.id %></td>
        <td><%= session.scope %></td>
        <td><%= session.strategy %></td>
        <td><%= session.success %></td>
        <td><%= session.failure_reason %></td>
        <td><%= session.user_type %></td>
        <td><%= session.context %></td>
        <td><%= session.ip  %></td>
        <td><%= session.user_agent %></td>
        <td><%= session.referrer %></td>
        <td><%= session.city %></td>
        <td><%= session.region %></td>
        <td><%= session.country %></td>
        <td><%= time_ago_in_words(session.created_at) %> ago</td>
      </tr>
    <% end %>
  </tbody>
</table>
